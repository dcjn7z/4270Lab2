lui $a1, 0xffff
addiu $a1, 0xffff
lui $zero, 0x0000
addiu $a0, $zero, 0
addiu $a2, $zero, 0
addiu $a3, $zero, 0
addiu $a4, $zero, 1
lui   $t1, 0x1001
sw    $a1, 0x0000 ($t1)
sw    $a0, 0x0004 ($t1)
addiu $a5, $zero, 0xA
addiu $a6, $zero, 0x0
bnq   $a6, $a3, 0xC
addiu $a1, $a1, 0x1           
addiu $a0, $a0, 0x1
bnq   $a6, $a3, 0x14
add $a2, $a1, $a0
sw  $a2, 0x8, ($t1)
addiu $a1, $a0, 0
addiu $a0, $a2, 0
addiu $a5, $a5, 0x1
bne $a6, $a5, 0x8028`
addiu $v0, $zero, 0xA
syscall
